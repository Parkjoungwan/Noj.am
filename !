#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
using namespace std;
 
void nojam1475(){
  string num;
  cin >> num ;
  int check[10]={0,};
  for(int i=0;i<num.size();i++){
    int N=num[i]-'0';
    check[N]++;
  }
  check[6]=(check[6]+check[9]+1)/2;
  int max=0;
  int maxnum=0;
  for(int i=0;i<9;i++){
    if(max<check[i]){
      max=check[i];
    }
  }
  cout << max;
}
void nojam12865(){
	int N,K;
	cin >> N;
	cin >> K;
	int Things[N][2];
	//things[N][0]은 무게 things[N][1]은 가치
	for(int i=0;i<N;i++){
		for(int j=0;j<2;j++){
			cin>>Things[i][j];
		}
	}
	dfsnojam(Things);	
	
}
void dfsnojam(int** Things){
	cout<<Things[0][0];

}

int main() {
  nojam12865();
}
